{
  "name": "@yagisumi/e7ipc2-types",
  "version": "0.1.0",
  "description": "TODO",
  "keywords": [],
  "engines": {
    "node": ">= 10.0.0"
  },
  "private": true,
  "main": "lib/e7ipc2-types",
  "module": "lib/e7ipc2-types.mjs",
  "types": "lib/e7ipc2-types.d.ts",
  "unpkg": "umd/e7ipc2-types.js",
  "scripts": {
    "test": "run-s prettier:check eslint:check mocha",
    "mocha": "cross-env TS_NODE_FILES=true TS_NODE_PROJECT=test/tsconfig.mocha.json mocha --config config/.mocharc.json \"test/**/*.test.ts\"",
    "mocha:coverage": "nyc --nycrc-path config/nyc.config.js npm run mocha -- --no-parallel",
    "build": "run-s clean build:*",
    "clean": "rimraf lib coverage umd *.tgz .rpt2_cache .nyc_output",
    "build:cjs": "rollup -c config/rollup.config.js -f cjs -o ./lib/e7ipc2-types.js",
    "build:mjs": "rollup -c config/rollup.config.js -f es -o ./lib/e7ipc2-types.mjs",
    "build:umd": "rollup -c config/rollup.config.umd.js",
    "build:d_ts": "tsc -p src -d --emitDeclarationOnly --outDir ./lib",
    "prettier:check": "prettier --check \"**/*.{ts,js,json}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,js,json}\"",
    "eslint:check": "eslint \"{src,test}/**/*.{js,ts}\"",
    "prepack": "run-s build test"
  },
  "author": "yagisumi",
  "repository": "github:yagisumi/node-e7ipc2-types",
  "homepage": "http://github.com/yagisumi/node-e7ipc2-types#readme",
  "license": "MIT",
  "prettier": "@yagisumi/my-config-prettier",
  "devDependencies": {
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@yagisumi/my-config-prettier": "^1.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "mocha": "^8.1.3",
    "mocha-ui-jest": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.29.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}
